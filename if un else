int main()
{
	string age, p = "abc123", p1, u = "abc", u1;
	cout << "Enter your age: ";
	cin >> age;
	cout << "Enter your username: ";
	cin >> u1;
	cout << "Enter password: ";
	cin >> p1;
	if (u1 == u && p1 == p) { cout << age; }
	else cout << "wrong username or password";
